<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.vamashikrishna.drawer.NextActivity">
    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/gate"
        android:layout_marginBottom="10dp"
        android:layout_marginRight="15dp"
        android:layout_marginLeft="10dp"
        android:src="@drawable/gate"/>
    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/vlcc"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="15dp"
        android:layout_marginBottom="10dp"
        android:src="@drawable/vlcc"
        android:layout_below="@+id/gate"/>
    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/ril"
        android:src="@drawable/ril"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="15dp"
        android:layout_marginBottom="10dp"
        android:layout_below="@+id/vlcc"/>
    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="15dp"
        android:id="@+id/rf"
        android:src="@drawable/rf"
        android:layout_marginBottom="10dp"
        android:layout_below="@+id/ril"/>
    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/gk"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="15dp"
        android:src="@drawable/gk"
        android:layout_below="@+id/rf"
        android:layout_marginBottom="10dp"/>
    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/ub"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="10dp"
        android:layout_toRightOf="@+id/gate"
        android:src="@drawable/ub"/>
    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/ab"
        android:src="@drawable/ab"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="10dp"
        android:layout_below="@+id/ub"
        android:layout_toRightOf="@+id/vlcc"/>
    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/ir"
        android:layout_toRightOf="@+id/ril"
        android:src="@drawable/ir"
        android:layout_below="@id/ab"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="10dp" />
    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/pfc"
        android:src="@drawable/pfc"
        android:layout_below="@+id/ir"
        android:layout_toRightOf="@+id/rf"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="10dp" />
    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/uti"
        android:src="@drawable/uti"
        android:layout_below="@+id/pfc"
        android:layout_toRightOf="@+id/gk"
        android:layout_marginLeft="20dp" />
    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/as"
        android:src="@drawable/as"
        android:layout_toRightOf="@+id/ub"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="35dp"/>
    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/ar"
        android:src="@drawable/ar"
        android:layout_below="@+id/as"
        android:layout_toRightOf="@id/pfc"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="35dp"/>
    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/lakme"
        android:src="@drawable/lakme"
        android:layout_below="@id/ar"
        android:layout_toRightOf="@id/uti"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="35dp"/>
</RelativeLayout>
